#include <stdio.h>
void swap(int x, int y) {
    int temp;
    temp = x;
    x = y;
    y = temp;

printf("\ninside swap function:\n");
printf("a = %d, b = %d\n", x, y);
}

int main() {
    int a, b;
printf("enter two numbers: ");
scanf("%d %d", &a, &b);
printf("\nbefore calling swap function:\n");
printf("a = %d, b = %d\n", a, b);
swap(a, b);
printf("\nafter calling swap function:\n");
printf("a = %d, b = %d\n", a, b);
return 0;
}
